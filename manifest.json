{
    "manifest_version" : 3,
    "name" : "Web Japanese Keyboard",
    "version": "1.0.1",
    "description": "japanese interactive keyboard for web pages",
    "permissions": [],
    "host_permissions": ["<all_urls>"],
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js" : ["layout/layout.js", "templates/template.js", "scripts/content.js"]
        }
    ]
}