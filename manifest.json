{
    "manifest_version" : 3,
    "name" : "japanese web keyboard",
    "version": "1.0.0",
    "description": "hiragana layout for web pages",
    "permissions": ["tabs"],
    "host_permissions": ["<all_urls>"],
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js" : ["layout/layout.js", "templates/template.js", "scripts/content.js"]
        }
    ]
}